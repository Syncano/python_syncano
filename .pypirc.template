[distutils] # this tells distutils what package indexes you can push to
index-servers =
  pypi
  pypitest

[pypi]
repository: https://pypi.python.org/pypi
username: {PYPI_USER}
password: {PYPI_PASSWORD}

[pypitest]
repository: https://testpypi.python.org/pypi
username: {PYPI_USER}
password: {PYPI_PASSWORD}
